#include <iostream>

using namespace std;

int main()
{
int n,arr[100],x,a;
cin>>n;
cin>>x;
for(int i=0;i<n;++i){
    cin>>arr[i];
}
int i;
for (i=0; i<n; i++)
    if (arr[i] == x)
        a=i;
 

    for (int j=a; j<n; j++){
         arr[j] = arr[j+1];
       
    }
      --n;
       
       for(int i=0;i<n;++i){
    cout<<arr[i];
}
